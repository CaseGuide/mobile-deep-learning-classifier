{
  "expo": {
    "name": "bird-app",
    "description": "A very interesting project.",
    "slug": "bird-app",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Used to snap and idenitfy food pictures",
        "NSPhotoLibraryUsageDescription": "Used to identify existing food pictures "
      },
      "bundleIdentifier": "com.rs-np.bird-classifier"
    },
    "android": {
      "package": "com.rsnp.birdclassifier",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL"
      ]

    }
  }
}